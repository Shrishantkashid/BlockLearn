{
  "version": 2,
  "public": false,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "api/[[...all]].js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "outputDirectory": "frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[[...all]].js"
    },
    {
      "src": "/test.html",
      "dest": "/public/test.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html"
    }
  ]
}